<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.jio.portlet.startups">
	<author>rajnikant.donga</author>
	<namespace>gns</namespace>

	<entity name="Startups" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="startupsId" type="long" primary="true" />
		
		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="companyName" type="String" />
		<column name="category" type="String" />
		<column name="startupImage" type="Long" />
		<column name="youtubeImages" type="Long" />
		<column name="youtubeLink" type="String" />
		<column name="shortDescription" type="String" />
		<column name="startupDesc" type="String" />
		<column name="longDescription" type="String" />
		<column name="twitter" type="String" />
		<column name="linkedIn" type="String" />
		<column name="youTube" type="String" />
		<column name="number" type="Long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

	</entity>
</service-builder>