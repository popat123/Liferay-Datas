<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.jio.portlet.startups.model.Startups" />
	<class name="com.jio.portlet.startups.model.impl.StartupsImpl" table="gns_Startups">
		<cache usage="read-write" />
		<id name="startupsId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="companyName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="category" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="startupImage" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="youtubeImages" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="youtubeLink" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="shortDescription" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="startupDesc" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="longDescription" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="twitter" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="linkedIn" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="youTube" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="number" type="com.liferay.portal.dao.orm.hibernate.LongType" column="number_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>