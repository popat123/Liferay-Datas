	<portlet>
		<portlet-name>web-content-management</portlet-name>
		<display-name>Web Content Management</display-name>
		<portlet-class>
			com.content.management.controller.WebContentManagementPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/webcontentmanagement/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Web Content Management</title>
			<short-title>Web Content Management</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>