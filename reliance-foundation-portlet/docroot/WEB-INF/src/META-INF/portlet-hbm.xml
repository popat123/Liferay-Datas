<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.jio.portlet.gallery.model.Gallery" />
	<class name="com.jio.portlet.gallery.model.impl.GalleryImpl" table="rf_Gallery">
		<cache usage="read-write" />
		<id name="galleryId" type="long">
			<generator class="increment" />
		</id>
		<property name="type" type="com.liferay.portal.dao.orm.hibernate.StringType" column="type_" />
		<property name="thumbnailName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mainCategory" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="projectImagesPath" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="galleryImagesId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="VideoImagesId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="thumbnailImagesId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="isVideo" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="isIMG" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="youTubeLink" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="videosImagPriority" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="gallryImagPriority" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="demoLink" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="liveLink" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="number" type="com.liferay.portal.dao.orm.hibernate.LongType" column="number_" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>